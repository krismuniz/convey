  "description": "Order Pizza Online",
  "main": "./app/index.js",
  "scripts": {
    "clean": "rm -rf ./app/dist",
    "heroku-postbuild": "npm run build",
    "build": "npm run clean && npm run build:js & npm run build:client",
    "build:js": "babel app/server -d app/dist",
    "build:client": "webpack -p",
    "dev": "npm run clean && npm run dev:js & npm run dev:client",
    "dev:js": "babel app/server -d app/dist --watch",
    "dev:client": "webpack --watch",
    "start": "node index",
    "local": "HOST_URI=http://convey.localtunnel.me npm start",
    "lt": "lt --port 8080 --subdomain convey"
  },
